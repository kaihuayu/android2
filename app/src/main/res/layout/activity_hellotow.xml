<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.flylsb.myyy.HellotowActivity">


    <Button
        android:id="@+id/btn_speak"
        android:layout_width="72dp"
        android:layout_height="42dp"
        android:onClick="Click_sqlite"
        android:text="sqlite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057" />

    <ListView
        android:id="@+id/db_listview"
        android:layout_width="351dp"
        android:layout_height="447dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/btOK_dialog"
        tools:layout_editor_absoluteX="17dp" />


    <Button
        android:id="@+id/btOK_dialog"
        android:layout_width="72dp"
        android:layout_height="42dp"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="32dp"
        android:onClick="Click_page"
        android:text="返回"
        app:layout_constraintStart_toEndOf="@+id/btn_speak"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>